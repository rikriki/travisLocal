{
  "name": "cool",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "rikriki <frederick.rosales@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "test-server": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --inline --content-base .",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.14",
    "puppeteer": "^1.13.0",
    "vue": "^2.6.8",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "chrome-remote-interface": "^0.27.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "jest": "^24.0.0",
    "jest-image-snapshot": "^2.8.1",
    "jest-puppeteer": "^3.9.0",
    "jest-serializer-vue": "^2.0.2",
    "mkdirp": "^0.5.1",
    "serviceworker-webpack-plugin": "^0.2.3",
    "vue-jest": "^3.0.2",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "jest": {
    "globalSetup": "<rootDir>/src/setup.js",
    "globalTeardown": "<rootDir>/src/teardown.js",
    "moduleFileExtensions": [
      "js",
      "vue",
      "json"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
